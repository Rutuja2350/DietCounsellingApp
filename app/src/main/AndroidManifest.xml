<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project3.onlinedietcounsellingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineDietCounsellingApp">
        <activity android:name=".MyProfile"></activity>
        <activity android:name=".payment.paymentPage" />
        <activity android:name=".payment.selectPlan" />
        <activity android:name=".Form" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Authentication.Login" />
        <activity android:name=".Authentication.Register" />
        <activity
            android:name=".splashScreen"
            android:theme="@style/splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>